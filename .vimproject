sent=/$PWD filter="*.R *.rb *.sh *.txt *.rake Rakefile" {
 setup.rb
 env.sh
 History.txt
 Manifest.txt
 PostInstall.txt
 Rakefile
 config=config {
  hoe.rb
  requirements.rb
 }
 install_scripts=install_scripts{
  analysis=analysis{
   Rakefile
  }
 }
 tasks=tasks {
  deployment.rake
  environment.rake
  website.rake
  config.rake
  install.rake
 }
 test=test {
  test_helper.rb
  test_sent.rb
 }
 bin=bin{
  sent.rb
 }
 R=R{
  matrix.R
 }
 lib=lib {
  sent.rb
  sent=sent {
   version.rb
  }
 }
 webservice=webservice{
  bin=bin{
   sentWS.rb
   test_sentWS.rb
  }
 }
 merb=merb{
  Rakefile
  config=config{
   dependencies.rb
   init.rb
   rack.rb
   router.rb
   environments=environments{
    development.rb
    production.rb
    rake.rb
    staging.rb
    test.rb
   }
  }
  lib=lib{
   helpers.rb
  }
  helpers=app/helpers{
   ajax_helper.rb
   global_helpers.rb
   main_helper.rb
   result_helper.rb
   test_helper.rb
  }
  controllers=app/controllers{
   main.rb
   result.rb
   normalize.rb
   ajax.rb
   help.rb
   exceptions.rb
   application.rb
  }
  views=app/views filter="*.rb *.haml *.mab *.erb"{
   normalize=normalize{
    index.html.haml
   }
   help=help{
    guide.html.haml
    help.html.haml
    glossary.html.haml
    ws.html.haml
   }
   result=result{
    _gene_details.html.haml
    _genecodis.html.haml
    _genes_list.html.haml
    _literature_header.html.haml
    _literature_table.html.haml
    _range_header.html.haml
    _words_list.html.haml
    complete_details.html.haml
    complete_literature.html.haml
    error.html.haml
    gene_literature.html.haml
    group_details.html.haml
    group_literature.html.haml
    main.html.haml
    wait.html.haml
   }
   main=main{
    index.html.haml
    error.html.haml
   }
   exceptions=exceptions{
    not_acceptable.html.erb
    not_found.html.erb
   }
   layout=layout{
    _google_analytics.html.haml
    application.html.haml
   }
  }
  models=app/models{ 
   group.rb
   job.rb
  }
  javascrips=public/javascripts filter="*.js"{
   sent.js
  }
  stylesheets=public/stylesheets/sass/ filter="*.sass"{
   sent.sass
  }
 }
}
