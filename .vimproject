rbbt-sent=/$PWD filter="*.R *.rb *.sh *.txt *.rake Rakefile" {
 README.rdoc
 Rakefile
 install_scripts=install_scripts{
  analysis=analysis{
   Rakefile
  }
 }
 tasks=tasks {
  install.rake
 }
 test=test {
  helper.rb
 }
 bin=bin{
  sent_config
 }
 R=R{
  matrix.R
 }
 lib=lib {
  sent.rb
  sent=sent {
   main.rb
  }
 }
}
